{
	"name": "github-timeline-api",
	"version": "1.0.0",
	"description": "Cloudflare Worker API for Repo Timeline Visualizer",
	"main": "src/index.ts",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"db:create": "wrangler d1 create repo_timeline",
		"db:migrate": "wrangler d1 migrations apply repo_timeline --remote",
		"db:migrate:local": "wrangler d1 migrations apply repo_timeline --local"
	},
	"dependencies": {
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241127.0",
		"typescript": "^5.7.2",
		"wrangler": "^3.94.0"
	}
}
